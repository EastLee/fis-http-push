{
  "extends": ["../.eslintrc.json"],
  "env": {
    "jest/globals": true
  },
  "plugins": [
    "jest",
    "@typescript-eslint"
  ],
  "rules": {
    "import/unambiguous": "off",
    "import/first": "off",
    "max-lines-per-function": "off",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "globals": {
    "myComponent": true,
    "san": true,
    "wrap": true,
    "clickInfo": true,
    "LI": true,
    "loadInvokeCount": true,
    "loadSuccess": true,
    "entityStr": true,
    "Label": true,
    "triggerEvent": true
  }
}
